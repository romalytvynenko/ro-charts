{
  "name": "ro-charts",
  "version": "0.1.0",
  "description": "Small Vue charting library powered by D3",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/rollup --config=rollup.config.js -w",
    "build": "./node_modules/.bin/rollup --config=rollup.config.js --environment PRODUCTION:true && ./node_modules/.bin/rollup --config=rollup.config.js --environment FORMAT:esm",
