  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romalytvynenko/ro-charts.git"
  },
  "keywords": [
    "vue",
    "charts",
    "d3"
  ],
  "author": "Roman Lytvynenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romalytvynenko/ro-charts/issues"
  },
  "homepage": "https://github.com/romalytvynenko/ro-charts#readme",
  "dependencies": {
    "d3": "^5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2"
  }
